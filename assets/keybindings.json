[
    {
        "key": "cmd+k",
        "command": "workbench.action.terminal.clear",
        "when": "terminalFocus"
    },
    {
        "key": "shift+cmd+/",
        "command": "workbench.action.openWorkspace"
    },
    {
        "key": "shift+alt+d",
        "command": "workbench.view.debug"
    },
    {
        "key": "shift+cmd+d",
        "command": "duplicate.execute",
        "when": "filesExplorerFocus"
    }
]